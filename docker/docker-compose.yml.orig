services:
  db:
    image: "mcr.microsoft.com/azure-sql-edge"
    environment:
      SA_PASSWORD: "${DATABASE_PASSWORD}"
      ACCEPT_EULA: "Y"
    ports:
      - "1433:1433"
    volumes:
      - ${MSSQL_PATH}:/var/opt/mssql
  web:
    image: "nginx:1.19.10-alpine"
    volumes:
      - ${WEBCONTENT_PATH}:/usr/share/nginx/html:ro
    ports:
      - "8081:80"
  cache:
    image: "memcached:1.6-alpine"
    ports:
      - "11211:11211"
